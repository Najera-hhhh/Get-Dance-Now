<head>
    <link rel="stylesheet" href="/css/AcademiasAll.css">
</head>
{{> navStudent}}
<div class="content mt-5">
    <div class="container">
        <div>
            <div class="card-body">
                <div class="row mt-5">
                    {{#each academias}}
                    <div class="col-lg-4" name="Target">
                        <div class="text-center card-box">
                            <div class="member-card pt-2 pb-2">
                                <div class="col">
                                    <h4 class="mr-4">{{nombre}}</h4>
                                </div>

                                <div class="col mt-4">
                                    <div class="row">
                                        <h4 class="mr-2">Número telefónico</h4>
                                    </div>
                                </div>
                                <p class="text-muted">
                                    {{numero}}
                                </p>
                                <div class="col">
                                    <div class="row">
                                        <h4 class="mr-2">Descripción</h4>
                                    </div>
                                </div>
                                <p class="text-muted">
                                    {{descripcion}}
                                </p>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>

            </div>
            <!-- container -->
        </div>
    </div>
</div>